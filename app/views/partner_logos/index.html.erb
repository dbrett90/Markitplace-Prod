<% provide(:title, "Our Partners") %>
<div class="parners-homepage">
    <div class="partners-homepage-2019">
        <div class="container-fluid" id="partner-offerings">
            <div class="row">
                <%  @partner_logos.each do |logo| %>
                    <div class = "shadow-bottom">
                        <div class = "col- 12 col-md-4 img-fluid">
                            <%= image_tag logo.thumbnail.variant(resize: "250x250"), class: "img-fluid" %>
                        </div>
                        <div class = "col-12 col-md-8 text-center">
                            <p class = "logo-text subtitle-description-mobile"> <%= logo.description %>
                        </div>
                    </div>
                <% end %>
            <div>
        </div>
    </div>
</div>